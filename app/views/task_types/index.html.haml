%h2 All Task Types

%table#tasktypes
  %thead
    %tr
      %th ID
      %th Name
      %th Batch Size
      %th More Info
  %tbody
    - @task_types.each do |t|
      %tr
        %td= t.id
        %td= t.name 
        %td= t.batch_size
        %td= link_to "More about #{t.name}", task_type_path(t)
%br
= link_to 'Add New Task Type', new_task_type_path
%br
%br
= link_to 'Tasks', tasks_path
%br
= link_to 'Elves', elves_path
